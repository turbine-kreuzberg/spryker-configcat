{
  "name": "turbine-kreuzberg/spryker-configcat",
  "description": "ConfigCat wrapper for spryker",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "preferred-install": "dist",
    "platform": {
      "php": "8.0.14"
    },
    "use-include-path": true,
    "sort-packages": true,
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  },
  "type": "library",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "TurbineKreuzberg\\": "src/TurbineKreuzberg/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Unit\\": "tests/Unit",
      "Functional\\": "tests/Functional",
      "Acceptance\\": "tests/Acceptance"
    }
  },
  "authors": [
    {
      "name": "Ben Kuhn",
      "email": "ben.kuhn@turbinekreuzberg.com"
    },
    {
      "name": "Edward Mateja",
      "email": "edward.mateja@turbinekreuzberg.com"
    },
    {
      "name": "Firat Cimen",
      "email": "firat.cimen@turbinekreuzberg.com"
    },
    {
      "name": "Christian Muellenhagen",
      "email": "christian.muellenhagen@turbinekreuzberg.com"
    },
    {
      "name": "Helder Correia",
      "email": "helder.correia@turbinekreuzberg.com"
    },
    {
      "name": "Serhii Nefodov",
      "email": "serhii.nefodov@turbinekreuzberg.com"
    }
  ],
  "require": {
    "php": ">=8",
    "spryker/kernel": "^3.54.0",
    "spryker/storage": "^3.18.0",
    "configcat/configcat-client": "^5.4.0",
    "psr/simple-cache": "^1.0.1"
  },
  "require-dev": {
    "codeception/codeception": "4.1.29",
    "codeception/module-asserts": "^1.0.0",
    "codeception/module-phpbrowser": "^1.0.0",
    "dg/bypass-finals": "^1.3",
    "phpstan/phpstan": "1.4.6",
    "spryker/code-sniffer": "0.17.4",
    "spryker/testify": "3.43.0"
  }
}
